            {% block body %}

            {% endblock %}
            <form id="whatsapp" class="wa-widget" action="00212601249246" data-chat="whatsapp"></form>
        </div>
    </div>
    <!--end page wrapper -->
    <!--start overlay-->
    <div class="overlay toggle-icon"></div>
    <!--end overlay-->
    <!--Start Back To Top Button--> <a href="javaScript:;" class="back-to-top"><i class='bx bxs-up-arrow-alt'></i></a>
