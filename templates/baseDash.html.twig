    <script>


        var chat = new WhatsAppWidget(document.getElementById('whatsapp'), {
            name: 'Yassine',
            division: '',
            photo: 'https://tajeer.online/assets/images/ecommerce/logo.png',
            introduction: 'Bonjour, comment pouvons-nous vous aider ?'
        }, [{
            data: 'name',
            type: 'text',
            required: true
        }, {
            data: 'email',
            type: 'email',
            required: true
        }, {
            data: 'message',
            type: 'text',
            required: true
        }]);
    </script>
    
    <script>
        var b = document.getElementsByClassName("wa-widget-toggle");
        var g = document.createElement('img');
        g.setAttribute("src", "https://cdn-icons-png.flaticon.com/512/174/174879.png");
        g.setAttribute("width", "32");
        g.setAttribute("height", "32");
        b[0].appendChild(g);
        
    </script>


